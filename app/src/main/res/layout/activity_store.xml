<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@drawable/rounded_navbar"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_menu" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOJA"
        android:textColor="@color/delfis01"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btTemas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.345" />

    <ImageView
        android:id="@+id/btTemas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/btPremium"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fundoitemloja" />

    <ImageView
        android:id="@+id/btPowerups"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="52dp"
        app:layout_constraintBottom_toTopOf="@+id/btMoedas"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/fundoitemloja" />

    <ImageView
        android:id="@+id/btPremium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="132dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toStartOf="@+id/btMoedas"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fundoitemloja" />

    <ImageView
        android:id="@+id/btMoedas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="130dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/fundoitemloja" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="98dp"
        android:layout_height="99dp"
        app:layout_constraintBottom_toBottomOf="@+id/btPowerups"
        app:layout_constraintEnd_toEndOf="@+id/btPowerups"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintStart_toStartOf="@+id/btPowerups"
        app:layout_constraintTop_toTopOf="@+id/btPowerups"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/fogo" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="101dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toBottomOf="@+id/btPremium"
        app:layout_constraintEnd_toEndOf="@+id/btPremium"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/btPremium"
        app:layout_constraintTop_toTopOf="@+id/btPremium"
        app:layout_constraintVertical_bias="0.041"
        app:srcCompat="@drawable/logopremium" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="83dp"
        android:layout_height="77dp"
        app:layout_constraintBottom_toBottomOf="@+id/btMoedas"
        app:layout_constraintEnd_toEndOf="@+id/btMoedas"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="@+id/btMoedas"
        app:layout_constraintTop_toTopOf="@+id/btMoedas"
        app:layout_constraintVertical_bias="0.219"
        app:srcCompat="@drawable/moedasvg" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="PREMIUM"
        android:textColor="@color/delfis01"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btPremium"
        app:layout_constraintStart_toStartOf="@+id/btPremium"
        app:layout_constraintTop_toBottomOf="@+id/imageView17" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="MOEDAS"
        android:textColor="@color/delfis01"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btMoedas"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="@+id/btMoedas"
        app:layout_constraintTop_toBottomOf="@+id/imageView18" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEMAS"
        android:textColor="@color/delfis01"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btTemas"
        app:layout_constraintEnd_toEndOf="@+id/btTemas"
        app:layout_constraintStart_toStartOf="@+id/btTemas"
        app:layout_constraintTop_toBottomOf="@+id/imageView21"
        app:layout_constraintVertical_bias="0.25" />

    <TextView
        android:id="@+id/textNome"
        android:layout_width="121dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:autoSizeTextType="none"
        android:text="POWER UPS"
        android:textColor="@color/delfis01"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btPowerups"
        app:layout_constraintHorizontal_bias="0.586"
        app:layout_constraintStart_toStartOf="@+id/btPowerups"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="99dp"
        android:layout_height="99dp"
        app:layout_constraintBottom_toBottomOf="@+id/btTemas"
        app:layout_constraintEnd_toEndOf="@+id/btTemas"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/btTemas"
        app:layout_constraintTop_toTopOf="@+id/btTemas"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pincel" />

</androidx.constraintlayout.widget.ConstraintLayout>